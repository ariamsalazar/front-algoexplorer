{
  "name": "front-algoexplorer",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://ariamsalazar.github.io/front-algoexplorer/",
  "dependencies": {
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.4.1",
    "create-react-class": "^15.6.3",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-d3-speedometer": "^0.9.0",
    "react-dom": "^16.12.0",
    "react-native-speedometer-chart": "^0.7.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "react-select": "^3.0.8",
    "react-vis": "^1.11.7",
    "reactstrap": "^8.4.1",
    "recharts": "^2.0.0-beta.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path .src/ --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recurisve",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "css-loader": "^3.4.2",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3"
  }
}
